<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smartlibrary.dao.definedSearchContentDao">
    <!--24小时刷新-->
    <cache flushInterval="86400000"/>
    <select id="getDefinedAcademyContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT id AS id,academy AS name FROM z_academu_org
    </select>
    <select id="getDefinedStudentStyleContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT id AS id,name AS name FROM z_identity
    </select>
    <select id="getDefinedStudentSexContent" resultType="com.smartlibrary.domain.DefinedSearchContent">

    </select>
    <select id="getDefinedPublisherContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT id AS id,publisher_name AS name FROM zz_book_publisher where publisher_name is not NULL
    </select>
    <select id="getDefinedBookStyleContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT DISTINCT id AS id,name AS name FROM z_book_category
    </select>

    <select id="getDefinedProfessionContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT DISTINCT dwjobTitle AS name FROM app_employee
        WHERE dwjobTitle is NOT NULL AND dwjobTitle !=''
    </select>
    <select id="getDefinedAssetTypeContent" resultType="com.smartlibrary.domain.DefinedSearchContent">

    </select>

    <select id="getDefinedPrintLocationContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT id AS id,name AS name FROM z_printcs_location
    </select>
    <select id="getDefinedPrintPaperTypeContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT id AS id,name AS name FROM z_printcs_papertype
    </select>
    <select id="getDefinedPrintTypeContent" resultType="com.smartlibrary.domain.DefinedSearchContent">
        SELECT id AS id,name AS name FROM z_printcs_printtype
    </select>
</mapper>